	"scripts": {
		"start": "cross-env NODE_PATH=src node scripts/start.js",
		"build": "cross-env NODE_PATH=src node scripts/build.js",
		"test": "node scripts/test.js --env=jsdom",
		"stat": "cross-env NODE_PATH=src NODE_ENV=production webpack --config ./config/webpack.config.prod.js --profile --json > stat.json",
		"deploy": "aws s3 sync ./build/ s3://tokennet-test-wallet.dev.blockchainos.org/ --acl public-read --delete --profile=boscoin"
	},
	"jest": {
		"collectCoverageFrom": [
			"src/**/*.{js,jsx}"
		],
		"setupFiles": [
			"<rootDir>/config/polyfills.js"
		],
		"testMatch": [
			"<rootDir>/src/**/__tests__/**/*.js?(x)",
			"<rootDir>/src/**/?(*.)(spec|test).js?(x)"
		],
		"testEnvironment": "node",
		"testURL": "http://localhost",
		"transform": {
			"^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
			"^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
			"^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
		},
		"transformIgnorePatterns": [
			"[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
		],
		"moduleNameMapper": {
			"^react-native$": "react-native-web"
		},
		"moduleFileExtensions": [
			"web.js",
			"js",
			"json",
			"web.jsx",
			"jsx",
			"node"
		]
	},
	"babel": {
		"presets": [
			"react-app"
		]
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"devDependencies": {
		"cross-env": "^5.0.5"
	},
	"optionalDependencies": {
		"ed25519": "0.0.4"
	}
}
